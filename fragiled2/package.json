{
  "name": "fragile-news-source",
  "version": "1.0.0",
  "description": "FNS - Fragile News Source: Matrix-themed news platform with Keisha AI bias analysis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build && cp -r build/* ../build/",
    "setup": "npm install && npm run db:init",
    "db:init": "node scripts/initDatabase.js",
    "import:news": "node scripts/importNews.js",
    "analyze:pending": "node scripts/analyzePending.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "news",
    "bias-detection",
    "ai-analysis",
    "racism",
    "white-supremacy",
    "keisha-ai",
    "matrix-theme"
  ],
  "author": "djangamane",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "node-html-parser": "^6.1.12",
    "rss-parser": "^3.13.0",
    "sentiment": "^5.0.2",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djangamane/fragile-news-source.git"
  },
  "bugs": {
    "url": "https://github.com/djangamane/fragile-news-source/issues"
  },
  "homepage": "https://github.com/djangamane/fragile-news-source#readme"
}
